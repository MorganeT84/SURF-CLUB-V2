{% extends 'Backoffice/baseBackoffice.html.twig' %}

{% block title %}Level
{% endblock %}

{% block body %}

	<div class="main">
		<h1>Level</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ level.id }}</td>
				</tr>
				<tr>
					<th>Slug</th>
					<td>{{ level.slug }}</td>
				</tr>
				<tr>
					<th>Name</th>
					<td>{{ level.name }}</td>
				</tr>
				<tr>
					<th>CreatedAt</th>
					<td>{{ level.createdAt ? level.createdAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
				<tr>
					<th>UpdatedAt</th>
					<td>{{ level.updatedAt ? level.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('app_backoffice_level_index') }}">back to list</a>

		<a href="{{ path('app_backoffice_level_edit', {'id': level.id}) }}">edit</a>

		{{ include('Backoffice/level/_delete_form.html.twig') }}
	{% endblock %}
