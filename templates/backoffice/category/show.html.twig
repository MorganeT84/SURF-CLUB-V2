{% extends 'Backoffice/baseBackoffice.html.twig' %}

{% block title %}Category
{% endblock %}

{% block body %}

	<div class="main">
		<h1>Category</h1>

		<table class="table">
			<tbody>
			 
				<tr>
					<th>Name</th>
					<td>{{ category.name }}</td>
				</tr>
				<tr>
					<th>CreatedAt</th>
					<td>{{ category.createdAt ? category.createdAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
				<tr>
					<th>UpdatedAt</th>
					<td>{{ category.updatedAt ? category.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('app_backoffice_category_index') }}">back to list</a>

		<a href="{{ path('app_backoffice_category_edit', {'id': category.id}) }}">edit</a>

		{{ include('Backoffice/category/_delete_form.html.twig') }}
	{% endblock %}
