{% extends 'Backoffice/baseBackoffice.html.twig' %}

{% block title %}Spot
{% endblock %}

{% block body %}

	<div class="main">
		<h1>Spot</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ spot.id }}</td>
				</tr>
				<tr>
					<th>Slug</th>
					<td>{{ spot.slug }}</td>
				</tr>
				<tr>
					<th>Name</th>
					<td>{{ spot.name }}</td>
				</tr>
				<tr>
					<th>Place</th>
					<td>{{ spot.place }}</td>
				</tr>
				<tr>
					<th>CreatedAt</th>
					<td>{{ spot.createdAt ? spot.createdAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
				<tr>
					<th>UpdatedAt</th>
					<td>{{ spot.updatedAt ? spot.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('app_backoffice_spot_index') }}">back to list</a>

		<a href="{{ path('app_backoffice_spot_edit', {'id': spot.id}) }}">edit</a>

		{{ include('Backoffice/spot/_delete_form.html.twig') }}
	{% endblock %}
